public public class CopyArray {
    public static void main(String[] args) {
        int[] arrA = {1, 2, 3, 4, 5};
        int[] arrB = new int[arrA.length];
        for (int i = 0; i < arrA.length; i++) {
            arrB[i] = arrA[i];
        }
        for (int num : arrB) {
            System.out.print(num + " ");
        }
        System.out.println();
    }
}
